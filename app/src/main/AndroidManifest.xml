<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelguideapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/travelguidemy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CovidCases"></activity>
        <activity
            android:name=".AlreadyVisited"
            android:label="Already visited List" />
        <activity
            android:name=".WantToVisit"
            android:label="Want to visit list" />
        <activity
            android:name=".CountryActivity"
            android:label="Country activity" />
        <activity
            android:name=".AllCountries"
            android:label="All countries list" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>